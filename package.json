{
    "name": "obsidian-lexicon-dictionary-plugin",
    "version": "1.0.0",
    "description": "Improve vocabulary by using flash card style cards",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "build": "npm run lint && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "dictionary",
        "lexicon",
        "vocabulary"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@eslint/json": "^0.14.0",
        "@types/node": "^16.11.6",
        "esbuild": "0.17.3",
        "eslint": "^9.15.0",
        "eslint-plugin-obsidianmd": "latest",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.50.1"
    }
}